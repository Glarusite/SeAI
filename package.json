  "scripts": {
    "api": "npx dotenv-cli@latest -e .env -- npx @rtk-query/codegen-openapi ./api.config.ts && eslint --fix ./src/store/api.ts",
    "build": "tsc",
    "clean": "git clean -fdx -e node_modules",
    "lint": "eslint --cache --fix .",
    "upgrade": "yarn upgrade-interactive && expo install --fix",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web --go",
    "export": "expo export -p web",
    "publish": "run export && aws s3 rm s3://seai.co --recursive && aws s3 cp ./dist s3://seai.co --recursive && aws cloudfront create-invalidation --distribution-id E1FIR9JYPYYW4O --paths /",
    "update": "npx dotenv-cli@latest -e .env -- npx eas-cli@latest update --auto"
  }
